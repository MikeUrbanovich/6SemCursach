@model _6SemCursach.Web.Models.CourceModel


<h2>Регистрация</h2>

<form asp-action="AddCource" asp-controller="Admin" asp-anti-forgery="true">
    <div class="validation" asp-validation-summary="ModelOnly"></div>
    <div>
        <div>
            <label asp-for="Title">Введите Имя</label><br />
            <input type="text" asp-for="Title" />
            <span asp-validation-for="Title" />
        </div>
        <div>
            <label asp-for="Price">Введите Email</label><br />
            <input type="number" min="0" asp-for="Price" />
            <span asp-validation-for="Price" />
        </div>
        <div>
            <input type="submit" value="Добавить" />
        </div>
    </div>
</form>

